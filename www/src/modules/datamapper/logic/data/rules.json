{
  "input": "http://schema.vostronet.com/#/definitions/user",
  "output": "http://schema.vostronet.com/#/definitions/simple-user",
  "connections": [{
    "id": "conn-1",
    "source": "input.firstName",
    "target": "1-js.input.firstName"
  }, {
    "id": "conn-2",
    "source": "input.lastName",
    "target": "1-js.input.lastName"
  }, {
    "id": "conn-3",
    "source": "1-js.output",
    "target": "output.name"
  }, {
    "id": "conn-4",
    "source": "input.id",
    "target": "output.id"
  }, {
    "id": "conn-5",
    "source": "input.devices",
    "target": "2-map.input"
  }, {
    "id": "conn-6",
    "source": "2-map.output",
    "target": "output.deviceIds"
  }],
  "components": [{
    "id": "1-js",
    "type": "exec",
    "input": {
      "lastName": {"type": "string"},
      "firstName": {"type": "string"}
    },
    "output": {
      "name": "string"
    },
    "script": "return {name: input.firstName + \" \" + input.lastName};"
  }, {
    "id": "2-map",
    "type": "map",
    "input": {
      "$ref": "http://schema.vostronet.com/#/definitions/device"
    },
    "output": {
      "type": "number"
    },
    "connections": [{
      "id": "conn-7",
      "source": "input.id",
      "target": "output"
    }]
  }]
}